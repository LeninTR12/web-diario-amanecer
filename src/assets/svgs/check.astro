---
import type { Icon } from "@/lib/types";

type Props = Icon;

const {
  width = "20",
  height = "20",
  color = "currentColor",
  strokeWidth = "1.5",
  size,
} = Astro.props;
---

<svg
  xmlns="http://www.w3.org/2000/svg"
  viewBox="0 0 24 24"
  width={size ? size : width}
  height={size ? size : height}
  fill="none"
  color={color}
>
  <path
    d="M0 0h24v24H0z"
    fill="none"
  />
  <path
    d="M5 12l5 5l10 -10"
    stroke="currentColor"
    stroke-width={strokeWidth}
    stroke-linecap="round"
    stroke-linejoin="round"
  />
</svg>